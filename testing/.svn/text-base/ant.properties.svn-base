# ---Project Name---
project_name=Testing
#
###############################################
# ---Compiler---
###############################################
javac.debug=on
javac.deprecation=on
javac.optimize=off
javac.verbose=off
###############################################
# ---Base Directory---
###############################################
base.dir=C:/development/esm-platform/Testing
base.lib.dir=${base.dir}/lib
project.dir=${base.dir}
###############################################
# ---JavaDoc---
###############################################
javadoc.dir=${base.dir}/javadoc
###############################################
# ---Logging Directory---
###############################################
logs.dir=${base.dir}/javadoc
#
###############################################
# ---Java EE---
###############################################
javaee.dir=C:/development/esm-platform/CommonLibraries/WebLogic92
javaee.jarfile=weblogic.jar
javaee.path=${javaee.dir}/${javaee.jarfile}
