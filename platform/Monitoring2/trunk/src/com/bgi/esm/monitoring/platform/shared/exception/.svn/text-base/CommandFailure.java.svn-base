package com.bgi.esm.monitoring.platform.shared.exception;

/**
 * Generic command failure
 * 
 * @author G.S. Cole (guycole at gmail dot com)
 */
public class CommandFailure extends EmsException {

	/**
	 * Generic command failure
	 */
	public CommandFailure() {
		super(COMMAND_FAILURE);
	}

	/**
	 * Command failure
	 */
	public CommandFailure(String arg) {
		super(arg);
	}

	/**
	 * Error String
	 */
	public static final String COMMAND_FAILURE = "Command Failure";

	/**
	 * Error String
	 */
	public static final String INSERT_UPDATE_FAILURE = "Insert/Update Failure";

	/**
	 * Error String
	 */
	public static final String QUEUE_FAILURE = "Queue Failure";

	/**
	 * Serial version identifier. 
	 * Be sure to update this when the class is updated.
	 */
	public static final long serialVersionUID = 1L;
}
