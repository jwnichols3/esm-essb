<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.bgi.esm.monitoring.splunk.splunk_send.server.data.orm.hibernate.HttpAttribute" table="http_attribute" >
        <id name="RowID" column="row_id" type="text"          >
            <generator class="uuid" />
        </id>
        <property name="RequestID"             type="text"         column="request_id"            />
        <property name="AttributeName"         type="text"         column="attribute_name"        />
        <property name="AttributePersistence"  type="text"         column="attribute_persistence" />
    </class>
</hibernate-mapping>