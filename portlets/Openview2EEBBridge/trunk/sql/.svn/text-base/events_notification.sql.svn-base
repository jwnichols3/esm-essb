DROP TABLE events_notification IF EXISTS;
CREATE TABLE events_notification
{
   row_id bigint identity,
   message_id     varchar(63),
   node_name      varchar(63),
   node_type      varchar(63),
   creation_date  varchar(63),
   creation_time  varchar(63),
   receive_date   varchar(63),
   receive_time   varchar(63),
   application    varchar(63),
   message_group  varchar(63),
   object         varchar(63),
   severity       varchar(63),
   operators      varchar(63),
   message text   text,
   instruction    text,
   attributes     text,
   suppress_count varchar(63),
   ovo_interface  varchar(63),
   method         varchar(63),
   PRIMARY KEY ( row_id )
};
