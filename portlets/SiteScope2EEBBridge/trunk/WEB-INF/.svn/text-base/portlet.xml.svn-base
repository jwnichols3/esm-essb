<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd" version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">
	<portlet>
		<portlet-name>SiteScope2EEBBridge</portlet-name>
		<display-name>ESM - SiteScope-to-EEB Bridge</display-name>
		<portlet-class>org.apache.portals.bridges.struts.StrutsPortlet</portlet-class>
		<init-param>
			<name>ServletContextProvider</name>
			<value>com.liferay.util.apache.bridges.struts.LiferayServletContextProviderWrapper</value>
		</init-param>
		<init-param>
			<name>ViewPage</name>
			<value>/view.do</value>
		</init-param>
		<init-param>
			<name>HelpPage</name>
			<value>/help.do</value>
		</init-param>
		<init-param>
			<name>EditPage</name>
			<value>/edit.do</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>edit</portlet-mode>
			<portlet-mode>help</portlet-mode>
		</supports>
		<portlet-info>
			<title>Enterprise Systems Management - HTTP Bridge for SiteScope to Enterprise Event Bus</title>
			<short-title>ESM - SiteScope-to-EEB Bridge</short-title>
			<keywords>Enterprise Systems Management, ESM, EEB, Enterprise Event Bus</keywords>
		</portlet-info>
	</portlet>
</portlet-app>
